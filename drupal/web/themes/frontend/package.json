{
  "name": "drupal-frontend",
  "version": "2.0.0",
  "description": "Frontend Starterkit for Drupal Projects",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack --progress --mode development --config webpack.config.dev.js --watch",
    "build": "webpack --progress --mode production --config webpack.config.build.js",
    "scaffold": "yo story",
    "lint": "lint-staged",
    "lint-all": "eslint \"src/**/*.js\" --fix && csscomb src --tty-mode",
    "pre-commit": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "email": "hello@saschaeggenberger.com",
  "author": "Sascha Eggenberger",
  "license": "MIT",
  "homepage": "https://sascha.is",
  "keywords": [
    "drupal",
    "webpack",
    "frontend",
    "boilerplate"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "^7.12.11",
    "@storybook/addon-actions": "^6.1.15",
    "@storybook/addon-essentials": "^6.1.15",
    "@storybook/addon-links": "^6.1.15",
    "@storybook/vue": "^6.2.0-alpha.19",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "10.0.1",
    "babel-loader": "^8.2.2",
    "babel-minify-webpack-plugin": "^0.3.1",
    "bourbon": "^5.1.0",
    "browser-sync": "^2.26.14",
    "browser-sync-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^3.6.0",
    "csscomb": "4.3.0",
    "es6-promise": "^4.1.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "0.12.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "expose-loader": "^0.7.5",
    "fibers": "^4.0.3",
    "file-loader": "^5.0.2",
    "generator-story": "file:.generator",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "img-loader": "^3.0.2",
    "lint-staged": "9.5.0",
    "lodash.throttle": "^4.1.1",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass-json-importer": "^4.3.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "svg-inline-loader": "^0.8.0",
    "svg-spritemap-webpack-plugin": "=3.7.1",
    "twig-loader": "^0.5.5",
    "vue-loader": "^16.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.19.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.4.0",
    "webpack-fix-style-only-entries": "^0.6.1",
    "webpack-merge": "^5.7.3",
    "webpack-modernizr-loader": "^5.0.0",
    "webpack-notifier": "^1.12.0",
    "yo": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "dependencies": {
    "@unic/composite-observer": "0.0.10",
    "ally.js": "^1.4.1",
    "core-js": "^3.8.3",
    "intersection-observer": "^0.12.0",
    "mdn-polyfills": "^5.19.0",
    "modernizr": "^3.11.4",
    "no-scroll": "^2.1.1",
    "node-sass-glob-importer": "^5.3.0",
    "normalize.css": "^8.0.0",
    "object-fit-images": "^3.2.4",
    "sass": "^1.32.5",
    "vanilla-lazyload": "^12.4.0",
    "vue": "^3.0.5",
    "whatwg-fetch": "^3.5.0"
  },
  "lint-staged": {
    "*.scss": [
      "csscomb --tty-mode",
      "git add"
    ],
    "*.js": [
      "eslint --fix \"{,**,**/**}*.js\"",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "github.com/saschaeggi/drupal-docksal-starterkit"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "ie > 10",
    "iOS > 9"
  ]
}
